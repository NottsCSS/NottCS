<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NottCS.Views.TestPage1"             
             xmlns:customControls="clr-namespace:NottCS.CustomControls;assembly=NottCS"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:NottCS.ViewModels;assembly=NottCS"
             xmlns:behaviors="clr-namespace:NottCS.Behaviors;assembly=NottCS"
             xmlns:converters="clr-namespace:NottCS.Converters;assembly=NottCS"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance viewModels:TestViewModel1}">
    <ContentPage.Content>
        <StackLayout>
            <customControls:DateTimePicker DateTime="{Binding DateTime1}"></customControls:DateTimePicker>
            <Button Command="{Binding AddCommand}"></Button>
            <Button Command="{Binding SomeCommand}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>